speed(5)
def bracelet ():
    for i in range(12):
        penup()
        forward(50)
        pendown()
        color("red")
        begin_fill()
        circle(10)
        end_fill()
        penup()
        setposition(0,0)
        left(10)
        penup()
        forward(50)
        pendown()
        color("blue")
        begin_fill()
        circle(10)
        end_fill()
        penup()
        setposition(0,0)
        left(10)
        penup()
        forward(50)
        pendown()
        color("purple")
        begin_fill()
        circle(10)
        end_fill()
        penup()
        setposition(0,0)
        left(10)
        
bracelet()

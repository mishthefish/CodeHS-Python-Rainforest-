speed(0)
square_length = int(input("what should be the sqaure_length ?"))
penup()
def draw():
    for i in range(4):
        pendown()
        forward(square_length)
        left(90)
        penup()
setposition(200-square_length,200-square_length)
draw()
setposition(200-square_length,-200)
draw()
setposition(-200,-200)
draw()
setposition(-200,200-square_length)
draw()

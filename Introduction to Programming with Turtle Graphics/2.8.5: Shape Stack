speed(0)
def draw_circle():
    pendown()
    circle(25)
    circle(25, 180)
def square():
    forward(25)
    right(90)
    for i in range(3):
     forward(50)
     right(90)
    forward(25)
    penup()
    right(90)
    forward(50)
    right(90)
    
penup()
setposition(0,-200)

def shape_stack():
    for i in range(4):
        draw_circle()
        square()
pendown()
shape_stack()
